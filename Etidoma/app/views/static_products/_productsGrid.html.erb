						<tr>
							<td>
								<div class="polaroid">
									<a href="/static_products/maquinas_etiquetadoras">
									<%= image_tag("maquinas_etiquetadoras.jpg", :title => " Ver Maquinas Etiquetadoras", :class => "image") %>
